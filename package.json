{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "vue server-side-render cli",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production --config build/webpack.prod.conf.js",
    "dev": "webpack-dev-server --mode development --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "client": "NODE_ENV=production webpack --mode production --config build/webpack.client.conf.js",
    "server": "webpack --mode production --config build/webpack.server.conf.js && node server/index.js",
    "ssr": "npm run client && npm run server"
  },
